 321   
 322   Equations
 323   
 324  *price block
 325     pmdef(i)         definition of domestic import prices
 326     pedef(i)         definition of domestic export prices
 327     absorption(i)    value of domestic sales
 328     sales(i)         value of domestic output
 329     actp(i)          definition of activity prices
 330     pkdef(i)         definition of capital goods price
 331     pindexdef        definition of general price level
 332   
 333  *output block
 334     activity(i)      production function
 335     profitmax(i,lc)  first order condition for profit maximum
 336     lmequil(lc)      labor market equilibrium
 337     cet(i)           cet function
 338     esupply(i)       export supply
 339     armington(i)     composite good aggregation function
 340     costmin(i)       f.o.c. for cost minimization of composite good
 341     xxdsn(i)         domestic sales for nontraded sectors
 342     xsn(i)           composite good agg. for nontraded sectors
 343   
 344  *demand block
 345     inteq(i)         total intermediate uses
 346     cdeq(i)          private consumption behavior
 347     dsteq(i)         inventory investment
 348     gdp              private gdp
 349     labory           total income accruing to labor
 350     capitaly         total income accruing to capital
 351     hhtaxdef         total household taxes collected by govt.
 352     gdeq             government consumption shares
 353     greq             government revenue
 354     tariffdef        tariff revenue
 355     premium          total import premium income
 356     indtaxdef        indirect taxes on domestic production
 357     netsubdef        export duties
 358   
 359  *savings-investment block
 360     hhsaveq          household savings
 361     gruse            government savings
 362     depreq           depreciation expenditure
 363     totsav           total savings
 364     prodinv(i)       investment by sector of destination
 365     ieq(i)           investment by sector of origin
 366   
 367  *balance of payments
 368     caeq             current account balance (bill dollars)
 369   
 370  *market clearing
 371     equil(i)         goods market equilibrium
 372   
 373  *objective function
 374     obj              objective function ;
 375   
 376   
 377  *price block
 378   
 379   pmdef(it)..       pm(it) =e= pwm(it)*er*(1 + tm(it) + pr) ;
 380   
 381   pedef(it)..       pe(it) =e= pwe(it)*(1 + te(it))*er ;
 382   
 383   absorption(i)..   p(i)*x(i) =e= pd(i)*xxd(i) + (pm(i)*m(i))$it(i) ;
 384   
 385   sales(i)..        px(i)*xd(i) =e= pd(i)*xxd(i) + (pe(i)*e(i))$it(i) ;
 386   
 387   actp(i)..         px(i)*(1-itax(i)) =e= pva(i) + sum(j, io(j,i)*p(j) ) ;
 388   
 389   pkdef(i)..        pk(i) =e= sum(j, p(j)*imat(j,i) );
 390   
 391   pindexdef..       pindex =e= sum(i, pwts(i)*p(i) ) ;
 392   
 393   
 394  *output and factors of production block
 395   
 396   activity(i)..     xd(i) =e= ad(i) * prod(lc$wdist(i,lc), l(i,lc)**alphl(i
      ,lc) )
 397                               *k(i)**(1 - sum(lc, alphl(i,lc)) ) ;
 398   
 399   profitmax(i,lc)$wdist(i,lc).. wa(lc)*wdist(i,lc)*l(i,lc) =e=
 400                                    xd(i)*pva(i)*alphl(i,lc) ;
 401   
 402   lmequil(lc)..     sum(i, l(i,lc)) =e= ls(lc) ;
 403   
 404   cet(it)..         xd(it) =e= at(it)*( gamma(it)*e(it)**rhot(it) +
 405                     ( 1-gamma(it) )*xxd(it)**rhot(it) )**(1/rhot(it)) ;
 406   
 407   esupply(it)..     e(it)/xxd(it) =e= ( pe(it)/pd(it)*(1 - gamma(it))/gamma
      (it) )
 408                                       **(1/(rhot(it)-1) ) ;
 409   
 410   armington(it)..   x(it) =e= ac(it)*(delta(it)*m(it)**(-rhoc(it)) +
 411                     (1-delta(it))*xxd(it)**(-rhoc(it)))**(-1/rhoc(it)) ;
 412   
 413   costmin(it)..     m(it)/xxd(it) =e= ( pd(it)/pm(it)*delta(it)/(1-delta(it
      )) )**
 414                                       (1/(1 + rhoc(it))) ;
 415   
 416   xxdsn(in)..       xxd(in) =e= xd(in) ;
 417   
 418   xsn(in)..         x(in) =e= xxd(in) ;
 419   
 420   
 421  *demand block
 422   
 423   inteq(i)..        int(i) =e= sum(j, io(i,j)*xd(j) );
 424   
 425   dsteq(i)..        dst(i) =e= dstr(i)*xd(i) ;
 426   
 427   cdeq(i)..         p(i)*cd(i) =e= sum(hh, cles(i,hh)*(1-mps(hh))*yh(hh)
 428                                            *(1-htax(hh)) ) ;
 429   
 430   gdp..             y =e= sum(hh, yh(hh) ) ;
 431   
 432   labory..          yh("lab-hh") =e= sum(lc, wa(lc)*ls(lc) ) + remit*er ;
 433   
 434   capitaly..        yh("cap-hh") =e= sum(i, pva(i)*xd(i) ) - deprecia -
 435                                 sum(lc, wa(lc)*ls(lc) ) + fbor*er + ypr ;
 436   
 437   hhsaveq..         hhsav =e= sum(hh, mps(hh)*yh(hh)*(1 - htax(hh))) ;
 438   
 439   greq..            gr =e= tariff - netsub + indtax +tothhtax ;
 440   
 441   gruse..           gr =e= sum(i, p(i)*gd(i)) + govsav ;
 442   
 443   gdeq(i)..         gd(i) =e= gles(i)*gdtot ;
 444   
 445   tariffdef..       tariff =e= sum(it, tm(it)*m(it)*pwm(it) )*er ;
 446   
 447   indtaxdef..       indtax =e= sum(i, itax(i)*px(i)*xd(i) );
 448   
 449   netsubdef..       netsub =e= sum(it, te(it)*e(it)*pwe(it) )*er ;
 450   
 451   premium..         ypr =e= sum(it, pwm(it)*m(it) )*er*pr ;
 452   
 453   hhtaxdef..        tothhtax =e= sum(hh, htax(hh)*yh(hh) ) ;
 454   
 455   depreq..          deprecia =e= sum(i, depr(i)*pk(i)*k(i) ) ;
 456   
 457   totsav..          savings =e= hhsav + govsav + deprecia + fsav*er ;
 458   
 459   prodinv(i)..      pk(i)*dk(i) =e= kio(i)*invest - kio(i)*sum(j, dst(j)*p(
      j)) ;
 460   
 461   ieq(i)..          id(i) =e= sum(j, imat(i,j)*dk(j));
 462   
 463  *balance of payments
 464   
 465   caeq..            sum(it, pwm(it)*m(it)) =e= sum(it, pwe(it)*e(it))
 466                                            +  fsav + remit + fbor ;
 467  *market clearing
 468   
 469   equil(i)..        x(i) =e= int(i) + cd(i) + gd(i) + id(i) + dst(i) ;
 470   
 471  *objective function
 472   
 473   obj..             omega =e= prod(i$cles(i,"lab-hh"),
 474                                          cd(i)**cles(i,"lab-hh")) ;
